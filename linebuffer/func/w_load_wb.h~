#include <stdint.h>
#include <algorithm>
#include <string>
#include <cstring>

typedef float DTYPE;

void w_load_wb(
	DTYPE const *stream_in,
	DTYPE **stream_out,
	int d0_size,
	int d1_size,
	int inch,
	int skip_size, // skip one element
	int st_point)
{

	for(int id_d0 = 0; id_d0 < d0_size; id_d0++){
		for(int id_d1 = 0; id_d1 < d1_size; id_d1++){
		  		DTYPE *tmp_ptr = stream_out[id_d0];
				tmp_ptr[id_d1] = stream_in[( (st_point+id_d0)*skip_size + inch )*d1_size + id_d1];
		}
	}

}
